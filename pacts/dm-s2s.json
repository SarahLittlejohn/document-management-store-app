{
  "provider": {
    "name": "s2s"
  },
  "consumer": {
    "name": "dm"
  },
  "interactions": [
    {
      "description": "4. lease request (testing)",
      "request": {
        "method": "POST",
        "path": "/lease",
        "headers": {
          "Content-Type": "application/json"
        }
      },
      "response": {
        "status": 200,
        "body": "s2s.s2s.s2s"
      }
    },
    {
      "description": "0. Incorrect Authorization header",
      "request": {
        "method": "GET",
        "path": "/details",
        "headers": {
          "Authorization": "x"
        }
      },
      "response": {
        "status": 403
      },
      "providerStates": [
        {
          "name": "provider returns a s2s service for a valid token"
        }
      ]
    },
    {
      "description": "1. Valid auth token",
      "request": {
        "method": "GET",
        "path": "/details",
        "headers": {
          "Authorization": "iQ.64a0.8"
        },
        "matchingRules": {
          "header": {
            "Authorization": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "(([a-z]|[A-Z]|[0-9])+)\\.(([a-z]|[A-Z]|[0-9])+)\\.(([a-z]|[A-Z]|[0-9])+)"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "response": {
        "status": 201,
        "body": "em_gw"
      },
      "providerStates": [
        {
          "name": "provider returns a s2s service for a valid token"
        }
      ]
    },
    {
      "description": "2. No auth token",
      "request": {
        "method": "GET",
        "path": "/details"
      },
      "response": {
        "status": 403
      },
      "providerStates": [
        {
          "name": "provider returns a s2s service for a valid token"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "3.6.10"
    }
  }
}